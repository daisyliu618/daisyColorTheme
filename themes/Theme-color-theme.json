{
  "name": "daisy theme",
  "type": "dark",
  "colors": {
  "activityBarBadge.background": "#F5CE16",
  "list.activeSelectionForeground": "#F5CE16",
  "list.inactiveSelectionForeground": "#FF4081",
  "list.highlightForeground": "#FF4081",
  "scrollbarSlider.activeBackground": "#FF408150",
  "editorSuggestWidget.highlightForeground": "#F5CE16",
  "textLink.foreground": "#FF4081",
  "progressBar.background": "#F5CE16",
  "pickerGroup.foreground": "#FF4081",
  "tab.activeBorder": "#FF4081",
  "notificationLink.foreground": "#FFFFFF",
  "editorWidget.resizeBorder": "#FF4081",
  "editorWidget.border": "#FF4081",
  "settings.modifiedItemIndicator": "#FF4081",
  "settings.headerForeground": "#FFFFFF",
  "panelTitle.activeBorder": "#FF4081",
  "breadcrumb.activeSelectionForeground": "#FF4081",
  "menu.selectionForeground": "#F5CE16",
  "menubar.selectionForeground": "#FF4081",
  "editor.findMatchBorder": "#FF4081",
  "selection.background": "#FF408140",
  "functions": "#8DDB1F",
  "strings": "#F6F489",
  "variables": "#F5CE16",
  "Class name":"#48ADC3",
  "keywords": "#F88F5F",
  "types": "#A5DD6E"

},

"tokenColors": [
  {
    "name": "Comment",
    "scope": "comment",
    "settings": {
    "foreground": "#4C380F",
    "fontStyle": "bold"
    }
    },
    {
      "name": "variable",
      "scope": "variables",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variable Instances",
      "scope": [
        "variable.instance",
        "variable.other.instance",
        "variable.readwrite.instance",
        "variable.other.readwrite.instance",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#7fdbca"
      }
    },

    {
        "name": "Support Constant Math",
        "scope": "support.constant.math",
        "settings": {
          "foreground": "#E80054"
        }
      },
  
      {
        "name": "Number",
        "scope": ["constant.numeric", "constant.character.numeric"],
        "settings": {
          "foreground": "#E80054",
          "fontStyle": ""
        }
      },
  
      {
        "name": "Built-in constant",
        "scope": [
          "constant.language",
          "punctuation.definition.constant",
          "variable.other.constant"
        ],
        "settings": {
          "foreground": "#82AAFF"
        }
      },
      {
        "name": "User-defined constant",
        "scope": ["constant.character", "constant.other"],
        "settings": {
          "foreground": "#E80054"
        }
      },
      {
        "name": "Colon in functions",
        "scope": "punctuation",
        "settings": {
          "foreground": "#9E6BF3"
        }
      },
      {
        "name": "Function name",
        "scope": "meta.function entity.name.function",
        "settings": {
          "foreground": "#F5CE16",
          "fontStyle": ""
        }
      },
      {
        "name": "Library class/type",
        "scope": ["support.type", "support.class"],
        "settings": {
          "foreground": "#FB002F"
        }
      },
      {
        "name": "Object",
        "scope": "object",
        "settings": {
          "foreground": "#FB002F"
        }
      },
      {
        "name": "Boolean",
        "scope": "constant.language.boolean",
        "settings": {
          "foreground": "#FB002F"
      }
    }

 ]
}

